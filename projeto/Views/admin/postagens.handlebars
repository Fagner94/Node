<h2>Lista de Usuarios</h2>
<hr>
<a href="/admin/postagens/add"><button class="btn btn-success">Nova Postagens</button></a></br>

{{#each postagens}}
<div class="card mt-4">

    <div class="card-body">
        <h4>{{titulo}}</h4>
        <p>Descrição:{{descricao}}</p>
        <small>Data:{{data}}</small>
        <small>Categoria:{{categoria.nome}}</small>
        </br>
        <a href="/admin/postagens/edit/{{_id}}"><button class="btn btn-success mt-4">Editar Postagens</button></a>
        <a href="/admin/postagens/deletar/{{_id}}"><button class="btn btn-danger mt-4">Deletar Postagens</button></a>


    </div>

</div>




{{else}}

<h4>Nenhuma postagem</h4>

{{/each}}